package com.lu;

public class ShapeFactory {
	public Shape getShape(String name) {
		
		
		
		
		if(name.equalsIgnoreCase("circle"))
		{
			return new Circle();
		}
		else if(name.equalsIgnoreCase("rectangle"))
		{
			return new Circle();
		}
		else if(name.equalsIgnoreCase("square"))
		{
			return new Circle();
		}
		
		return null;

	}

}
